                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table table-hover" id="prodTable">
                            <thead>
                                <tr class="text-left">
                                    <th  style="padding-left: 1.5rem;">Image</th>
                                    <th style="padding-left: 2rem;">Name</th>
                                    <th style="padding-left: 1.5rem;">Category</th>
                                    <th style="padding-left: 1.5rem;">Price</th>
                                    <th style="padding-left: 1.5rem;">Quantity</th>
                                    <th style="padding-left: 1.5rem;">Discount</th>
                                    <th style="padding-left: 1.5rem;">Update</th>
                                    <th style="padding-left: 1.5rem;">Status</th>
                                </tr>
                            </thead>
                            <tbody>
                                <% if (typeof productData !== 'undefined') { %>
                               <% for( let i = 0; i < productData.length; i++ ) { %>
                            
                                    <tr>
                                        <td style="vertical-align: middle;" width="10%">
    
                                            <% if (Array.isArray(productData[i].image) && productData[i].image.length > 0) { %>
                                                <img class="img-thumbnail" width="80" src="/public/uploads/<%= productData[i].image[0] %>" alt="no image available">
                                            <% } else if (productData[i].image) { %>
                                                <img class="img-thumbnail" width="80" src="/public/uploads/<%= productData[i].image %>" alt="no image available">
                                            <% } else { %>
                                                <span>No image available</span>
                                            <% } %>
                                        </td>
                                        
                                          
                                        <td style="vertical-align: middle; padding-left: 1.5rem;" width="17%"><%= productData[i].name %></td>
                                        
                                        <td style="vertical-align: middle; padding-left: 1.5rem;" width="12%">
                                            <% if (!productData[i].category) { %>
                                                <span class="text-danger">No Category</span>
                                            <% } else { %>
                                                <% if (Array.isArray(productData[i].category)) { %>
                                                    <% for (let j = 0; j < productData[i].category.length; j++) { %>
                                                        <%= productData[i].category[j].name %>
                                                        <% if (j < productData[i].category.length - 1) { %>, <% } %>
                                                    <% } %>
                                                <% } else { %>
                                                    <%= productData[i].category.name %>
                                                <% } %>
                                            <% } %>
                                           
                                        </td>
                                        
                                        <td style="vertical-align: middle; padding-left: 2rem;" width="12%">₹<%= productData[i].price %></td>
                                        <td style="vertical-align: middle; padding-left: 2.5rem;" width="12%"><%= productData[i].quantity %></td>
                                        <td style="vertical-align: middle; padding-left: 2.5rem;" width="12%"><%= productData[i].discount %>%</td>
                                      
                                        <td style="vertical-align: middle; padding-left: 1.5rem;" width="10%"><a href="/admin/editproduct/<%= productData[i]._id %>" class="btn btn-success btn-sm rounded font-sm mt-15">Edit</a></td>
                                         
                                       <td>
                                         
                                          <% if (productData[i].product_status) { %>
                                                <!-- Display unblock button -->
                                             <br> <button onclick="listUnlistprod('<%= productData[i]._id %>')" type="button" class="btn  btn-outline-success rounded font-sm">List</button></br>
                                             <% } else { %>
                                                <!-- Display block button -->
                                                <br><button onclick="listUnlistprod('<%= productData[i]._id %>')" type="button" class="btn btn-outline-danger rounded font-sm">Unlist</button></br>
                                            <% } %>
                                        </td>
            
                                    </tr>
                                
        
                                <% } %>
                                <% } %>
                                
                            </tbody>
                        </table> <!-- table-responsive.// -->
                    </div>
