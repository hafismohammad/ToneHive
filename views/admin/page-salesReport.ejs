                                            <th>Coupon</th>
                                        </tr>
                                    </thead>
                                    <!-- Table Body -->
                                    <tbody>
                                        <% for (let i = 0; i < orders.length; i++) { %>
                                            <tr>
                                                <td><%= i + 1 %></td>
                                                <td><%= orders[i].orderId %></td>
                                                <td><%= orders[i].userId.name %></td>
                                                <td><%= orders[i].createdAt.toLocaleDateString() %></td>
                                                <td>
                                                    <% for (let j = 0; j < orders[i].products.length; j++) { %>
                                                        <%= orders[i].products[0].productName %><br>
                                                    <% } %>
                                                </td>
                                                <td>
                                                    <% for (let j = 0; j < orders[i].products.length; j++) { %>
                                                        <%= orders[i].products[j].quantity %><br><br>
                                                    <% } %>
                                                </td>
                                                <td><%= orders[i].paymentMethod %></td> 
                                                <td><%= orders[i].totalPrice %></td>
                                                <td>
                                                    <% for (let j = 0; j < orders[i].products.length; j++) { %>
                                                        <%= orders[i].products[j].productId.discount %><br><br>
                                                    <% } %>
                                                </td>
                                                <% if(orders[i].coupon){ %>
                                                    <td><%= orders[i].coupon %></td>
                                                <% } else { %>
                                                    <td>none</td>
                                                <% } %>
                                            </tr>
                                        <% } %>
                                    </tbody>
                                </table>
