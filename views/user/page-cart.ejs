            </td>
            <td class="price" data-title="Price">
                <span>₹ <%= cartItem.productDetails.price %> </span>
            </td>
            <td class="text-center" data-title="Quantity">
                <div>
                    <button class="cart-item-count mr-5" onclick="changeQuantity('<%= cartItem._id %>','<%= cartItem.productDetails._id %>', -1)" <%= cartItem.items && cartItem.items.quantity === 1 ? 'disabled' : '' %>>-</button>
                   
                    <span class="qty-val"><%= cartItem.items ? cartItem.items.quantity : 0 %></span>
                    <button class="cart-item-count ml-5" onclick="changeQuantity('<%= cartItem._id %>','<%= cartItem.productDetails._id %>', 1)">+</button>
                </div>
            </td>
            <td class="text-right" data-title="Subtotal">
                <span>₹ <%= cartItem.subtotal %> </span>
            </td>
            <td class="action" data-title="Remove">
                <a  class="text-muted" onclick="productRemove('<%= cartItem.productDetails._id %>')"><i class="fi-rs-trash"></i></a>
            </td>
        </tr>
        <% }) %>
