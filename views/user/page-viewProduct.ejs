<script>
    function addtoCartBtn(prodId){
        console.log(prodId);
        swal("Adding to Cart", "Please wait...", "info"); 
        
        $.ajax({
            url: '/addToCart/'+ prodId,
            method: 'get'
        }).done((res) => {
            console.log(res);
            swal("Success", "Product added to cart!", "success").then(() => {
                // Reload the page after the user closes the success alert
                window.location.reload();
            });
        }).fail((error) => {
    console.error("Error adding to cart:", error);
    
    swal({
        title: 'Error',
        text: 'Quantity exceeds available stock.',
        icon: 'warning'
    });
});

    }
</script>

<script>
function addToWishlist(prodId) {
    console.log(prodId);
    fetch(`/addWishlist/${prodId}`, { method: 'GET' })
    .then(response => {
        if (response.ok) {
            return response.json(); // Parse response body as JSON
        } else {
            throw new Error('Network response was not ok');
        }
    })
    .then(data => {
        console.log(data); // Log the JSON response data
        swal("Good job!", "Item added to wishlist", "success");
        setTimeout(() => {
            window.location.reload();
        }, 1000);
    })
    .catch(error => {
        console.error('There was an error with the fetch operation:', error);
        swal( "Product already exists in the wishlist");
    });
}


</script>



