<div class="card-header">
    <div class="table-responsive">
        <table>
            <tbody>
                <% if(wishlist.length > 0) { %>
                    <% wishlist.forEach(wish => { %>
                        <% wish.product.forEach(product => { %>
                            <tr>
                                <td class="text-center">
                                    <img width="80px" src="/public/uploads/<%= product.image[0] %>" alt="">
                                    <p><%= product.name %></p>
                                </td>
                                <td>₹ <%= product.offerPrice %></td>
                                <td class="text-center">
                                    <button onclick="addtoCartBtn('<%= product._id %>')" id="addToCartBtn" type="submit" class="btn btn-primary">
                                        <i class="fi-rs-cart"></i>Add to cart
                                    </button>
                                </td>
                                <td class="text-center">
                                    <button onclick="removeWishlist('<%= product._id %>')" id="Wishlist" type="submit" class="btn btn-danger ">
                                        <i class="fi-rs-heart"></i> Remove from wishlist
                                    </button>
                                </td>
                            </tr>
                        <% }) %>
                    <% }) %>
                <% } else { %>
                    <tr>
                        <div class="mt-50 mb-50">
                            <td class="text-center" style="font-size: 20px;">Your wishlist is empty</td>
                        </div>
                    </tr>
                    
                <% } %>
            </tbody>
            
        </table>
    </div> 
